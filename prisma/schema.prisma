// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Document {
  id           String @id @default(cuid())
  title        String
  content      String @default("")
  bibliography String @default("")
}
